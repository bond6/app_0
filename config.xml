<?xml version='1.1' encoding='utf-8'?>
<widget android-versionCode="10028" id="io.nardus.ica_citrus_mrl" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <icon density="mdpi" height="57" platform="android" src="www/img/logo.png" width="57"/>
    <name>Citrus MRL</name>
    <description>
        ICA citrus post-harvest fungicide choices for different export markets.
    </description>
    <author email="serfonteinnardus@gmail.com">
        Nardus Serfontein
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.3.0" />
    <engine name="browser" spec="^5.0.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
</widget>
